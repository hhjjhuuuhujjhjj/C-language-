//swapping of two numbers 
#include<stdio.h>
int main ()
{
	int a,b,temp;
	printf("enter two numbers");
	scanf ("%d %d",&a,&b);
	
	temp = a;
	a=b;
	
	b=temp;
	
	printf("after swapping a = %d b =%d\n",a,b);
	return 0;
	}
//factors of a number 
#include<stdio.h>
int main ()
{
	int n,i ;
	printf("enter a number :");
	scanf("%d",&n);
	
	printf("factors of the number");
	for (i=1;i<=n;i++)
	{
		if(n%i==0)
		printf("%d ",i);
		
	}
	return 0;
}
