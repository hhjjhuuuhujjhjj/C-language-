#include <stdio.h>
int main() {
    int a = 5, b = 3;

    if (a != b) {           // Outer if
       
         printf("both are eqaul\n");
          
                if (a > b)
                 {        // Inner if
            printf("a is larger\n");
        } else {
            printf("b is larger\n");
        }
    } else {
        printf("Both are equal\n");
    }

    return 0;
}
